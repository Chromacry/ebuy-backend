{
  "name": "ebuy-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "src/index.mjs",
  "c8": {
    "include": [
      "src/**/*.mjs",
      "ui_test/*.mjs"
    ],
    "exclude": [
      "tests/*.mjs",
      "src/routes/*.mjs",
      "src/index.mjs"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "text",
      "html",
      "cobertura"
    ],
    "check-coverage": true,
    "all": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  },
  "scripts": {
    "test:userController": "c8 mocha 'tests/UserController.test.mjs'",
    "test:reviewController": "c8 mocha 'tests/ReviewController.test.mjs'",
    "test:productController": "c8 mocha tests/ProductController.test.mjs",
    "test:productControllerBuild": "mocha tests/ProductController.test.mjs",
    "test:productPage": "mocha tests/ManageProductPage.test.mjs",
    "test:orderController": "c8 mocha 'tests/OrderController.test.mjs'",
    "coverage": "c8 mocha tests/*.test.mjs",
    "browsertest": "mocha tests/HomePage.test.mjs",
    "reviewtest": "c8 mocha ui_test/ReviewsSelenium.test.mjs",
    "carttest": "c8 mocha ./ui_tests/ManageCart.test.mjs",
    "manageordertabletest": "c8 mocha ./ui_tests/ManageOrderTable.test.mjs",
    "manageproducttabletest": "c8 mocha ./ui_tests/ManageProductTable.test.mjs",
    "producttest": "c8 mocha ./ui_tests/ProductDetails.test.mjs",
    "edge-carttest": "c8 mocha ./ui_tests/cross_browser/UIEdge_ManageCart.test.mjs",
    "edge-manageordertabletest": "c8 mocha ./ui_tests/cross_browser/UIEdge_ManageOrderTable.test.mjs",
    "edge-manageproducttabletest": "c8 mocha ./ui_tests/cross_browser/UIEdge_ManageProductTable.test.mjs",
    "edge-productdetailtest": "c8 mocha ./ui_tests/cross_browser/UIEdge_ProductDetails.test.mjs",
    "usertest": "c8 mocha ui_test/UserSelenium.test.mjs",
    "ui-coverage": "c8 mocha ui_test/*test.mjs",
    "test": "mocha tests/*.test.mjs",
    "start": "node src/index.mjs",
    "dev": "nodemon"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.4.0",
    "chromedriver": "^121.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-status-monitor": "^1.3.4",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.3",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "prom-client": "^15.1.0",
    "selenium-webdriver": "^4.17.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "c8": "^8.0.1",
    "chai": "^4.3.10",
    "mocha": "^10.2.0"
  }
}
